<%= form_for(interaction) do |f| %>
  <% if interaction.errors.any? %>
    <div id="error_explanation">
      <h2><%= pluralize(interaction.errors.count, "error") %> prohibited this interaction from being saved:</h2>

      <ul>
      <% interaction.errors.full_messages.each do |message| %>
        <li><%= message %></li>
      <% end %>
      </ul>
    </div>
  <% end %>

  <div class="field">
    <%= f.label :question_id %>
    <%= f.text_field :question_id %>
  </div>

  <div class="field">
    <%= f.label :response %>
    <%= f.number_field :response %>
  </div>

  <div class="field">
    <%= f.label :prompt_time %>
    <%= f.datetime_select :prompt_time %>
  </div>

  <div class="field">
    <%= f.label :thinking_milis %>
    <%= f.number_field :thinking_milis %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>
